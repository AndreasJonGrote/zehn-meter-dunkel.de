{
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "clean": "rm -rf dist/*",
    "dev": "parcel watch ./css/main.css ./js/main.js --no-cache --dist-dir ./dist --public-url .",
    "build": "npm run clean && parcel build --no-content-hash --no-cache --no-source-maps ./css/main.css ./js/main.js --dist-dir ./dist --public-url .",
    "staging": "npm run build && bash deploy-staging.sh",
    "production": "npm run build && bash deploy-production.sh"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "parcel": "^2.12.0",
    "postcss": "^8.4.41",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.4.10"
  },
  "dependencies": {
    "rfs": "^9.0.6",
    "swiper": "^11.0.0"
  }
}